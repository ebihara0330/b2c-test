{
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "aadb2c": {
          "registration": {
            "clientIdSettingName": "B2C_ClientID",
            "clientCredential": {
              "clientSecretSettingName": "B2C_Client_Secret"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://eleventyb2c.b2clogin.com/eleventyb2c.onmicrosoft.com/B2C_1_eleventy/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": [],
            "loginParameterNames": []
          }
        }
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/b2c",
      "statusCode": 302
    }
  }
}
